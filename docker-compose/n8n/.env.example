# .env.example

# General config
N8N_ENCRYPTION_KEY=your_32_character_encryption_key_here_replace_me_12345678
APIFY_API_KEY=apify_api_your_apify_api_key_here_replace_with_real_key
MCP_BEARER_TOKEN=your_bearer_token_here_replace_me
GENERIC_TIMEZONE=Asia/Bangkok
N8N_RUNNERS_ENABLED=true
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
BROWSERLESS_TOKEN=your_browserless_token_here_replace_me

# Webhook / networking
N8N_HOST=your-domain.com
N8N_PROTOCOL=http
WEBHOOK_URL=https://your-domain.com/

# Uncomment and fill if using basic auth
# N8N_BASIC_AUTH_ACTIVE=true
# N8N_BASIC_AUTH_USER=your-n8n-username
# N8N_BASIC_AUTH_PASSWORD=your-n8n-password