# Copy this file to .env and fill in your actual values

# Core Server Authentication
MCP_BEARER_TOKEN=your-mcp-bearer-token-here
HOST_AGENT_BEARER_TOKEN=your-host-agent-token-here

# n8n Configuration
N8N_BASE_URL=https://n8n.correlion.ai
N8N_API_KEY=your-n8n-api-key-here

# LangSmith Tracing Configuration
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your-langsmith-api-key-here
LANGSMITH_PROJECT=ai-dev-server

# OpenAI-Compatible API for 11Labs
OPENAI_COMPATIBLE_API_KEY=sk-your-custom-key-here

# OAuth (if needed)
OAUTH_CLIENT_SECRET=your-oauth-client-secret-here